<?xml version="1.0" encoding="UTF-8"?>

<!-- Gramática DTD asociada -->
<!DOCTYPE Vuelos [
	<!ELEMENT Vuelos (Vuelo*)>
	<!ELEMENT Vuelo (origen, destino)>
	<!ELEMENT origen (#PCDATA)>
	<!ELEMENT destino (#PCDATA)>
]>

<!-- Ejemplo -->
<Vuelos>
	<Vuelo>
		<origen>Sevilla (SEV)</origen>
		<destino>Londres Standed (LSD)</destino>
	</Vuelo>
	<Vuelo>
		<origen>Jerez de la Frontera (XRZ)</origen>
		<destino>Berlín Schönefeld (SFX)</destino>
	</Vuelo>
</Vuelos>